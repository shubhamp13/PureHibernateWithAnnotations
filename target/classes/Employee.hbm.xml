<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="dto.Employee">
    <id name="empId" column="EMPID"></id>
    <property name="empName" column="EMP_NAME"></property>
    <property name="empAddress" column="EMP_ADD"></property>
    <property name="empPhoneNo" column="EMP_PHONE"></property>
    <property name="empEmailId" column="EMP_EMAIL"></property>
    <property name="empSalary" column="EMP_SAL"></property>
    
    </class>
    </hibernate-mapping>